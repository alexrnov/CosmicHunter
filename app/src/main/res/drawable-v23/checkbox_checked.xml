<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="0dp" />
            <size
                android:height="40dp"
                android:width="40dp" />
            <solid android:color="@color/noActivityButtonPressSolidStartEnd"/>
            <stroke android:width="3dp" android:color="@color/buttonDefaultCenter" />
        </shape>
    </item>


    <item
        android:width="14dp"
        android:height="6dp"
        android:top="14dp"
        android:right="14dp"
        android:gravity="center">
        <rotate
            android:fromDegrees="30">
            <shape android:shape="rectangle">
                <solid android:color="@color/buttonDefaultCenter"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="28dp"
        android:height="5dp"
        android:left="9dp"
        android:top="1dp"
        android:gravity="center"
        >
        <rotate
            android:fromDegrees="-60">
            <shape android:shape="rectangle">
                <solid android:color="@color/buttonDefaultCenter"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="40dp"
        android:height="40dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>

</layer-list>

